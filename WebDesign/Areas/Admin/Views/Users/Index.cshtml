<a href="/Admin/Users/Insert" class="btn btn-primary">Yeni Ekle</a>
<table class="table table-bordered" style="margin-top:20px;">

    <thead>
        <tr>
            <td>Id</td>
            <td>İsim</td>
            <td>E-Posta</td>
            <td>Şifre</td>
            <td>Yetki</td>
            <td>Menu</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Password</td>
                <td>@item.Roles</td>
                <td>
                    <a href="/Admin/User/Update/@item.Id" class="btn btn-primary">Güncelle</a>
                    <a href="/Admin/User/Delete/@item.Id" class="btn btn-danger">Sil</a>
                </td>
            </tr>
        }
    </tbody>
</table>